<% @users.each do |user| %>
<div class="d-flex p-2 flex-row">
  <% if !(user.in?(current_user.following)) && current_user != user %>
  <%= image_tag('tuitter-egg.jpg', size: "48x48", class: "mr-2 img-circle") %>
  <h6 class="text-capitalize rt-bar"><%= user.fullname %></h6>
    <% end %>
</div>
<% end %>